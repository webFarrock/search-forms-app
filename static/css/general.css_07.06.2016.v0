/* Berry */
.berry-input {
  position: relative;
  overflow: hidden;
  display: inline-block;
  vertical-align: top;
}
.berry-input input {
  position: absolute;
  top: 0px;
  left: -9999px;
}
.berry-input label {
  cursor: pointer;
  margin: 0;
  display: block;
  position: relative;
  z-index: 1;
}
.berry-input b {
  display: block;
  position: absolute;
  top: 0;
  z-index: 0;
}
.-berry-status-disabled label {
  cursor: default;
}
.-berry-status-disabled b {
  cursor: default;
}
.-berry-direction-ltr label {
  padding-left: 20px;
}
.-berry-direction-ltr b {
  left: 0;
}
.-berry-direction-rtl label {
  padding-right: 20px;
}
.-berry-direction-rtl b {
  right: 0;
}
/* Custom styles */
.-berry-custom {
  line-height: 18px;
}
.-berry-custom b {
  width: 16px;
  height: 16px;
  border: 1px solid #d9d9d9;
  background: #fff;
  margin-top: 1px;
}
.-berry-custom b:before {
  content: '';
  display: block;
  position: absolute;
  top: 50%;
  left: 50%;
}
.-berry-custom label {
  font-size: 13px;
  text-transform: none;
}
.-berry-custom.-berry-type-checkbox b {
  border-radius: 3px;
}
.-berry-custom.-berry-type-checkbox b:before {
  width: 8px;
  height: 8px;
  margin: -4px 0 0 -4px;
}
.-berry-custom.-berry-type-checkbox.-berry-checked b:before {
  /* background:#a9bed0; */
  background: url('../i/checkbox.png') 50% 50% no-repeat;
}
.-berry-custom.-berry-type-radio b {
  border-radius: 50%;
}
.-berry-custom.-berry-type-radio b:before {
  border-radius: 50%;
  width: 4px;
  height: 4px;
  margin: -2px 0 0 -2px;
}
.-berry-custom.-berry-type-radio.-berry-checked b:before {
  background: #a9bed0;
}
.-berry-custom.-berry-status-disabled label {
  color: #ccc;
}
.-berry-custom.-berry-status-disabled b {
  background: #ccc;
  border-color: #ccc;
}
.-berry-custom.-berry-status-disabled.-berry-checked b:before {
  background: #fff;
}
.-berry-custom.-berry-empty {
  height: 16px;
}
.-berry-custom.-berry-empty b {
  top: 0;
  margin-top: 0;
}
.-berry-custom.-berry-empty label {
  padding-left: 16px;
  height: 16px;
}
.-berry-custom a {
  text-decoration: none;
}
.-berry-custom a:hover {
  text-decoration: underline;
}
