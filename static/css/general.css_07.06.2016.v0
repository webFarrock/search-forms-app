  width: 7px;
  height: 7px;
  left: -3px;
  bottom: -3px;
  background: #fff;
  border-radius: 50%;
  z-index: 2;
}
.timeline__dates__item span:after {
  width: 13px;
  height: 13px;
  left: -6px;
  bottom: -6px;
  border: 1px solid #fff;
  border-radius: 50%;
  z-index: 1;
}
