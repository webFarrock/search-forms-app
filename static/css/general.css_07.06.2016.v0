  height: 14px;
  background: url('../i/timeline.gif') 10px 0 repeat-x;
  margin-top: 7rem;
  margin-bottom: 2rem;
}
.timeline__dates {
  position: relative;
}
.timeline__dates__item {
  position: absolute;
  text-align: right;
  font-size: 12px;
  color: #fff;
  width: 34px;
  bottom: 16px;
  cursor: pointer;
}
.timeline__dates__item:before,
.timeline__dates__item:after {
  content: '';
  display: block;
  position: absolute;
  z-index: 3;
}
.timeline__dates__item:before {
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background: #fff;
}
.timeline__dates__item:after {
  width: 1px;
  left: 0;
  top: 100%;
  height: 16px;
  background: #fff;
}
.timeline__dates__item span {
  display: block;
  position: relative;
  width: 100%;
